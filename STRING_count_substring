// count all substring in a String
import java.util.*;
public class Main
{
    public static void printsubstr(String str)
    {
        int c=0;
    int n= str.length();
          
        for(int i=0;i<n;i++)
          {
            for(int j=i;j<n;j++)
            {
                for(int k=i;k<=j;k++)
                {
                   c++;
                }
               
            }
          }
           System.out.println(c);
       
    }
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    String str= sc.next();
	    printsubstr(str);

	}
}

