import java.util.*;
public class Main
{
    public static void palindrome(String str)
    {
        	String s="";
		for(int i=str.length()-1;i>=0;i--)
		{
		   s=s+str.charAt(i); 
		}
       boolean res=(str.equals(s)) ? true:false;
        System.out.println(res);
    }
   
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    String str= sc.next();
	   palindrome(str);
	   

	}
}
